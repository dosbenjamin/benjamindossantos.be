{
  "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "layouts/*": ["src/layouts/*"],
      "queries/*": ["src/queries/*"],
      "types/*": ["src/types/*"],
      "graphql-client": ["src/graphql-client"],
      "cloudinary": ["src/cloudinary"],
      "public/*": ["public/*"]
    },
    "module": "ESNext",
    "lib": ["ESNext", "WebWorker"],
    "types": ["vite/client", "node"],
    "allowJs": true
  }
}
